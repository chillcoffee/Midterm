import requests


def get_questions():
    parameters = {
        "amount": 10,
        "difficulty": 'easy',
        "type": 'boolean',
    }
    response = requests.get("https://opentdb.com/api.php?", params=parameters)
    response.raise_for_status()
    data = response.json()
    return data["results"]


